{
  "defaultSeverity": "error",
  "extends": [],
  "jsRules": {},
  "rules": {
      "no-var-requires": true,
      "await-promise": [true, "Thenable"],
      "curly": true,
      "no-conditional-assignment": true,
      "no-duplicate-super": true,
      "no-duplicate-switch-case": true,
      "no-eval": true,
      "use-isnan": true,
      "eofline": true,
      "indent": [true, "spaces", 2],
      "no-duplicate-imports": true,
      "prefer-const": true,
      "align": [true, "parameters", "statements", "members", "elements"],
      "arrow-return-shorthand": true,
      "class-name": true,
      "comment-format": [true, "check-space"],
      "encoding": true,
      "file-header": [true, "Copyright \\d{4} Caicloud authors. All rights reserved."],
      "import-spacing": true,
      "newline-before-return": true,
      "no-boolean-literal-compare": true,
      "no-consecutive-blank-lines": true,
      "no-redundant-jsdoc": true,
      "no-trailing-whitespace": [true, "ignore-comments"],
      "one-variable-per-declaration": [true, "ignore-for-loop"],
      "quotemark": [true, "single", "jsx-double"],
      "semicolon": [true, "always", "ignore-interfaces", "ignore-bound-class-methods"],
      "space-before-function-paren": [true, "always"],
      "space-within-parens": 1,
      "trailing-comma": [
        true,
        {
          "multiline": {
            "objects": "always",
            "arrays": "always",
            "functions": "never",
            "typeLiterals": "ignore"
          },
          "esSpecCompliant": true
        }
      ],
      "whitespace": [true, "check-branch", "check-decl", "check-operator", "check-module"]
  },
  "rulesDirectory": "./src"
}